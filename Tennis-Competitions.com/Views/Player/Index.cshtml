@model ICollection<PlayerServiceModel>

<h1>Players</h1>
<br />

<div class="row row-cols-1 row-cols-md-2 g-4">
    @{
        foreach (var player in Model)
        {
            <div class="col">
                <partial name="_PlayerPartial" model="@player" />
            </div>
        }
    }
</div>